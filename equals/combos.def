// Left-hand combos
COMB(atsign,    KC_AT,      KC_W,           KC_F)
COMB(lbrace,    KC_LCBR,    KC_F,           KC_P)
COMB(exclaim,   KC_EXLM,    KC_W,           LCTL_T(KC_R))
COMB(hash,      KC_HASH,    KC_F,           LALT_T(KC_S))
COMB(dollar,    KC_DLR,     KC_P,           LGUI_T(KC_T))
COMB(grave,     KC_GRV,     LSFT_T(KC_A),   LCTL_T(KC_R))
COMB(colon,     KC_COLN,    LCTL_T(KC_R),   LALT_T(KC_S))
COMB(lparen,    KC_LPRN,    LALT_T(KC_S),   LGUI_T(KC_T))
COMB(tilde,     KC_TILD,    KC_Z,           KC_X)
COMB(percent,   KC_PERC,    LCTL_T(KC_R),   KC_X)
COMB(circ,      KC_CIRC,    LALT_T(KC_S),   KC_C)
COMB(amper,     KC_AMPR,    LGUI_T(KC_T),   KC_D)
COMB(semi,      KC_SCLN,    KC_X,           KC_C)
COMB(lbrack,    KC_LBRC,    KC_C,           KC_D)

//Right-hand combos
COMB(rbrace,    KC_RCBR,    KC_L,           KC_U)
COMB(dquote,    KC_DQUO,    KC_U,           KC_Y)
COMB(equal,     KC_EQL,     KC_L,           LGUI_T(KC_N))
COMB(pipe,      KC_PIPE,    KC_U,           LALT_T(KC_E))
COMB(bslash,    KC_BSLS,    KC_Y,           LCTL_T(KC_I))
COMB(rparen,    KC_RPRN,    LGUI_T(KC_N),   LALT_T(KC_E))
COMB(minus,     KC_MINS,    LALT_T(KC_E),   LCTL_T(KC_I))
COMB(plus,      KC_PLUS,    LCTL_T(KC_I),   LSFT_T(KC_O))
COMB(aster,     KC_ASTR,    LGUI_T(KC_N),   KC_H)
COMB(langle,    KC_LABK,    LALT_T(KC_E),   KC_COMM)
COMB(rangle,    KC_RABK,    LCTL_T(KC_I),   KC_DOT)
COMB(rbrack,    KC_RBRC,    KC_H,           KC_COMM)
COMB(under,     KC_UNDS,    KC_COMM,        KC_DOT)
COMB(question,  KC_QUES,    KC_DOT,         KC_SLSH)
